{
  "name": "@graphql-mesh/http-handler",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "Urigo/graphql-mesh",
    "directory": "packages/http-handler"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "graphql": "*"
  },
  "dependencies": {
    "@graphql-mesh/fusion-runtime": "^0.0.0",
    "@graphql-tools/federation": "^1.1.10",
    "@graphql-tools/utils": "^10.0.8",
    "@whatwg-node/server": "^0.9.16",
    "graphql-yoga": "^5.0.0"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  },
  "sideEffects": false,
  "buildOptions": {
    "./package.json": "./package.json"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
